{
  "name": "asciify",
  "version": "0.1.0",
  "description": "A plug-in that transforms all images into their ascii-art equivalent. Uses canvas.",
  "repository": {
    "type": "git",
    "url": "https://github.com/apathetic/asciify.git"
  },
  "keywords": [
    "ascii",
    "canvas",
    "easter egg"
  ],
  "authors": [
    "wes hatch",
    "tim mcduff"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run build && nodemon index.js --exec babel-node",
    "build": "webpack --progress --bundle true",
    "watch": "babel src/demo.js --watch --out-file demo/js/ascii.js",
    "lint": "eslint src/*.js"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^2.8.0",
    "nodemon": "^1.9.1"
  }
}
